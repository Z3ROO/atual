{
  "version": 2,
  "builds": [
    {
      "src": "back/index.js",
      "use": "@vercel/node",
      "config": {
        "command": "cd front && npx vite build && mv dist ../back/ && cd ../back && npx tsc && cd ..",
        "output": "back"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/back/index.js"
    }
  ]
}